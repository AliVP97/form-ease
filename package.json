{
  "name": "form-ease",
  "version": "0.0.1",
  "description": "Create Forms as fast as possible powered from react hook form",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliVP97/form-ease.git"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "forms",
    "form-validation",
    "validation",
    "typescript",
    "react-hooks"
  ],
  "author": "Ali Valipour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AliVP97/form-ease/issues"
  },
  "homepage": "https://github.com/AliVP97/form-ease#readme",
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-multi-date-picker": "^4.3.1",
    "sass": "^1.68.0"
  }
}
